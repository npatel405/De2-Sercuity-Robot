           2F6 : 0020;  -- Mask5:    DW &B00100000
           2F7 : 0040;  -- Mask6:    DW &B01000000
           2F8 : 0080;  -- Mask7:    DW &B10000000
           2F9 : 00FF;  -- LowByte:  DW &HFF      ; binary 00000000 1111111
           2FA : 000F;  -- LowNibl:  DW &HF       ; 0000 0000 0000 1111
           2FB : 03C1;  -- OneMeter: DW 961       ; ~1m in 1.04mm units
           2FC : 01E1;  -- HalfMeter: DW 481      ; ~0.5m in 1.04mm units
           2FD : 024A;  -- TwoFeet:  DW 586       ; ~2ft in 1.04mm units
           2FE : 0125;  -- OneFoot:  DW 293       ; ~2ft in 1.04mm units
           2FF : 005A;  -- Deg90:    DW 90        ; 90 degrees in odometer units
           300 : 00B4;  -- Deg180:   DW 180       ; 180
           301 : 010E;  -- Deg270:   DW 270       ; 270
           302 : 0168;  -- Deg360:   DW 360       ; can never actually happen; for math only
           303 : 0064;  -- FSlow:    DW 100       ; 100 is about the lowest velocity value that will move
           304 : FF9C;  -- RSlow:    DW -100
           305 : 015E;  -- FMid:     DW 350       ; 350 is a medium speed
           306 : FEA2;  -- RMid:     DW -350
           307 : 01F4;  -- FFast:    DW 500       ; 500 is almost max speed (511 is max)
           308 : FE0C;  -- RFast:    DW -500
           309 : 008C;  -- MinBatt:  DW 140       ; 14.0V - minimum safe battery voltage
           30A : 1190;  -- I2CWCmd:  DW &H1190    ; write one i2c byte, read one byte, addr 0x90
           30B : 0190;  -- I2CRCmd:  DW &H0190    ; write nothing, read one byte, addr 0x90
  END;
